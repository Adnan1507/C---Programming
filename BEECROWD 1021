#include<stdio.h>
#include<math.h>
int main()
{
    double x, z;
    int a, b, c, d, e, f, g, h, i, j, k ,l, m, n, y, o, p, q, r, s, t, u, v, w;
    scanf("%lf", &x);
    a = x/100;
    b = fmod(x, 100);
    c = b/50;
    d = fmod(b, 50);
    e = d/20;
    f = fmod(d, 20);
    g = f/10;
    h = fmod(f, 10);
    i = h/5;
    j = fmod(h, 5);
    k = j/2;
    l = fmod(j, 2);
    m = l/1;
    n = fmod(l, 1);
    y = x;
    z = (x - y)*100;
    o = z/50;
    p = fmod(z, 50);
    q = p/25;
    r = fmod(p, 25);
    s = r/10;
    t = fmod(r, 10);
    u = t/5;
    v = fmod(t, 5);
    w = v/1;
    printf("NOTAS:\n%d nota(s) de R$ 100.00\n%d nota(s) de R$ 50.00\n%d nota(s) de R$ 20.00\n", a, c, e);
    printf("%d nota(s) de R$ 10.00\n%d nota(s) de R$ 5.00\n%d nota(s) de R$ 2.00\n", g, i, k);
    printf("MOEDAS:\n%d moeda(s) de R$ 1.00\n%d moeda(s) de R$ 0.50\n%d moeda(s) de R$ 0.25\n", m, o, q);
    printf("%d moeda(s) de R$ 0.10\n%d moeda(s) de R$ 0.05\n%d moeda(s) de R$ 0.01\n", s, u, w);
    return 0;
}
