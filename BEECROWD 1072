#include<stdio.h>
int main()
{
    int n, a, b, c, d, i;
    scanf("%d", &n);
    b=0;
    c=0;
    i=1;
    while(i<=n)
    {
        scanf("%d", &a);
        if(a>=10&&a<=20)
        {
            b++;
        }
        else
        {
            c++;
        }
        i++;
    }
    printf("%d in\n%d out\n", b, c);

    return 0;
}
