#include<stdio.h>
int main()
{
    int a, b, c, d;
    scanf("%d%d%d", &a, &b);
    if(a<b)
    {
        c = b - a;
        printf("O JOGO DUROU %d HORA(S)\n", c);
    }
    else if(a>b)
    {
        d = (24 - a) + b;
        printf("O JOGO DUROU %d HORA(S)\n", d);
    }
    else
    {
        printf("O JOGO DUROU 24 HORA(S)\n");
    }
    return 0;
}
