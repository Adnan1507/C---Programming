#include<stdio.h>
int main()
{
    double A, B, C, a, b, c, d, e, pi;
    scanf("%lf%lf%lf", &A, &B, &C);
    pi = 3.14159;
    a = (A * C)/2.0;
    b = pi * C * C;
    c = ((A + B)/2.0) * C;
    d = B * B;
    e = A * B;
    printf("TRIANGULO: %.3lf\nCIRCULO: %.3lf\nTRAPEZIO: %.3lf\nQUADRADO: %.3lf\nRETANGULO: %.3lf\n", a, b, c, d, e);
    return 0;
}
