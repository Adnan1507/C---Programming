#include<stdio.h>
int main()
{
    int i, j, n;
    n=1;
    i=1;
    while(n<=9)
    {
        j=7;
        if(i%2!=0)
        {
           printf("I=%d J=%d\n", i, j);
           j--;
           printf("I=%d J=%d\n", i, j);
           j--;
           printf("I=%d J=%d\n", i, j);
        }
        i++;
        n++;
    }
    return 0;
}
